package module1;

public class CPU {
    double price;

    class Processor {
        double cores;
        String manufactures;

        double getCache(double f) {
            return f;
        }
    }

    static class RAM {
        double memory;
        String manufactures;

        double getClockspeed(double r) {
            return r;
        }
    }
}

package module1;

import java.util.Scanner;

public class CpuDetails {

    public static void main(String[] args) {
        double p, m;
        Scanner s = new Scanner(System.in);

        System.out.print("Enter the processor cache: ");
        p = s.nextDouble();

        System.out.print("Enter the clock speed: ");
        m = s.nextDouble();

        CPU cpu = new CPU();
        CPU.Processor processor = cpu.new Processor();
        CPU.RAM ram = new CPU.RAM();

        System.out.println("Processor Cache = " + processor.getCache(p));
        System.out.println("RAM Clock speed = " + ram.getClockspeed(m));

        s.close();
    }
}
